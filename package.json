{
	"name": "gridify.it",
	"version": "0.0.0-dev",
	"private": true,
	"type": "module",
	"license": "AGPL-3.0-or-later",
	"description": "A web app to add a grid over images with customizable options",
	"author": {
		"name": "Edoardo Scibona",
		"url": "https://github.com/velut"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/velut/gridify.it.git"
	},
	"scripts": {
		"prepare": "svelte-kit sync || echo ''",
		"dev": "vite dev",
		"build": "vite build && bun run scripts/third-party-licenses.ts",
		"start": "vite build && vite preview",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"bunny:deploy": "bun run scripts/deploy-on-bunny.ts",
		"pre-push": "bun i && bun run lint && bun run check && bun run build"
	},
	"devDependencies": {
		"@iconify-json/material-symbols": "^1.2.34",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.38.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.13",
		"@types/bun": "^1.2.21",
		"@types/node": "^24.3.1",
		"@zip.js/zip.js": "^2.8.2",
		"attr-accept": "^2.2.5",
		"basecoat-css": "^0.3.2",
		"clsx": "^2.1.1",
		"event-to-files": "^0.3.0",
		"fast-equals": "^5.2.2",
		"file-selector": "^2.1.2",
		"p-map": "^7.0.3",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"rollup-plugin-license": "^3.6.0",
		"rollup-plugin-visualizer": "^6.0.3",
		"runed": "^0.31.1",
		"svelte": "^5.38.10",
		"svelte-check": "^4.3.1",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.13",
		"typescript": "^5.9.2",
		"typescript-svelte-plugin": "^0.3.50",
		"unplugin-icons": "^22.2.0",
		"vite": "^7.1.5",
		"vite-plugin-devtools-json": "^1.0.0",
		"zod": "^4.1.7"
	}
}
