{
	"name": "gridify.it",
	"description": "A web app to add a grid over images with customizable options",
	"version": "0.0.1",
	"license": "AGPL-3.0-or-later",
	"private": true,
	"author": {
		"name": "Edoardo Scibona",
		"url": "https://github.com/velut"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/velut/gridify.it.git"
	},
	"scripts": {
		"prepare": "svelte-kit sync",
		"dev": "tsx scripts/ensure-licenses-json.ts && vite dev",
		"build": "tsx scripts/ensure-licenses-json.ts && cross-env GENERATE_LICENSES=true vite build && vite build",
		"build:only": "vite build",
		"start": "tsx scripts/ensure-licenses-json.ts && vite build && vite preview",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"licenses:ensure": "tsx scripts/ensure-licenses-json.ts",
		"licenses:generate": "cross-env GENERATE_LICENSES=true vite build",
		"bunny:deploy": "tsx scripts/deploy-on-bunny.ts"
	},
	"dependencies": {
		"@square/svelte-store": "^1.0.17",
		"attr-accept": "^2.2.2",
		"client-zip": "^2.4.4",
		"file-saver": "^2.0.5",
		"file-selector": "^0.6.0",
		"p-limit": "^4.0.0",
		"pathe": "^1.1.1",
		"sveltekit-superforms": "1.5.0",
		"vanilla-cookieconsent": "^2.9.2",
		"zod": "^3.22.2"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.23.0",
		"@tailwindcss/typography": "^0.5.9",
		"@types/file-saver": "^2.0.5",
		"@types/html-minifier": "^4.0.2",
		"@types/klaw": "^3.0.3",
		"@types/node": "^20.5.6",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"autoprefixer": "^10.4.15",
		"cross-env": "^7.0.3",
		"daisyui": "^3.6.4",
		"dotenv": "^16.3.1",
		"eslint": "^8.48.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"hasha": "^5.2.2",
		"html-minifier": "^4.0.0",
		"klaw": "^4.1.0",
		"modern-normalize": "^2.0.0",
		"postcss": "^8.4.28",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.4.1",
		"rollup-plugin-license": "^3.0.1",
		"svelte": "^3.59.2",
		"svelte-check": "^3.5.0",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"tsx": "^3.12.7",
		"typescript": "^5.2.2",
		"vite": "^4.4.9",
		"vitest": "^0.33.0"
	},
	"type": "module"
}
