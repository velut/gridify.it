{
	"name": "pic-to-grid",
	"description": "A website to add a grid over images with customizable options",
	"version": "0.0.1",
	"license": "AGPL-3.0-or-later",
	"private": true,
	"author": {
		"name": "Edoardo Scibona",
		"url": "https://github.com/velut"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/velut/pic-to-grid.git"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "cross-env GENERATE_LICENSES=true vite build && vite build",
		"start": "vite build && vite preview",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"generate:licenses": "cross-env GENERATE_LICENSES=true vite build"
	},
	"dependencies": {
		"@square/svelte-store": "^1.0.15",
		"attr-accept": "^2.2.2",
		"client-zip": "^2.4.3",
		"file-saver": "^2.0.5",
		"file-selector": "^0.6.0",
		"p-limit": "^4.0.0",
		"pathe": "^1.1.0",
		"sveltekit-superforms": "^0.8.6",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.18.0",
		"@tailwindcss/typography": "^0.5.9",
		"@types/file-saver": "^2.0.5",
		"@typescript-eslint/eslint-plugin": "^5.59.6",
		"@typescript-eslint/parser": "^5.59.6",
		"autoprefixer": "^10.4.14",
		"cross-env": "^7.0.3",
		"daisyui": "^2.51.6",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"modern-normalize": "^2.0.0",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"rollup-plugin-license": "^3.0.1",
		"svelte": "^3.59.1",
		"svelte-check": "^3.3.2",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.2",
		"typescript": "^5.0.4",
		"vite": "^4.3.8",
		"vitest": "^0.31.1"
	},
	"type": "module"
}
