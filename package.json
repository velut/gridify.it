{
	"name": "gridify.it",
	"version": "0.0.0-dev",
	"private": true,
	"type": "module",
	"license": "AGPL-3.0-or-later",
	"description": "A web app to add a grid over images with customizable options",
	"author": {
		"name": "Edoardo Scibona",
		"url": "https://github.com/velut"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/velut/gridify.it.git"
	},
	"scripts": {
		"prepare": "svelte-kit sync || echo ''",
		"dev": "vite dev",
		"build": "vite build && bun run scripts/third-party-licenses.ts",
		"start": "vite build && vite preview",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"bunny:deploy": "bun run scripts/deploy-on-bunny.ts",
		"pre-push": "bun i && bun run lint && bun run check && bun run build"
	},
	"devDependencies": {
		"@iconify-json/material-symbols": "^1.2.28",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.22.2",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.11",
		"@types/bun": "^1.2.17",
		"@types/node": "^24.0.4",
		"@zip.js/zip.js": "^2.7.62",
		"attr-accept": "^2.2.5",
		"clsx": "^2.1.1",
		"daisyui": "^5.0.43",
		"file-selector": "^2.1.2",
		"p-map": "^7.0.3",
		"prettier": "^3.6.1",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.13",
		"rollup-plugin-license": "^3.6.0",
		"svelte": "^5.34.8",
		"svelte-check": "^4.2.2",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.8.3",
		"unplugin-icons": "^22.1.0",
		"vite": "^7.0.0",
		"vite-plugin-devtools-json": "^0.2.0"
	}
}
